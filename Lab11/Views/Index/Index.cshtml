@{
    ViewData["Title"] = "MyApp";
}

<style>
    body {
        font-family: Georgia, serif;
        background-color: #1e1e1e;
        color: #fff;
        margin: 0;
        padding: 0;
    }
    .main {
        padding: 50px;
        max-width: 800px;
        margin: 0 auto;
    }
    h1 {
        font-size: 36px;
        margin-bottom: 20px;
        color: #f3c623;
    }
    ul {
        padding-left: 20px;
    }
    li {
        font-size: 18px;
        margin-bottom: 10px;
    }
    p {
        font-size: 20px;
        margin-top: 20px;
    }
</style>


<div class="main">
        <h1>Actions Logging:</h1>
        <ul>
            @foreach (var logEntry in ViewBag.LogEntries)
            {
                <li>Action '@logEntry.Method' (@logEntry.Datetime.ToString("yyyy/MM/dd HH:mm:ss"))</li>
            }
        </ul>
        
    <h1>Unique users:</h1>
    <p>@ViewBag.UniqueUsersCount</p>
</div>
